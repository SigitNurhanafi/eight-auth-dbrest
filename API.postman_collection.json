    {
      "name": "Login User",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Extract token from response",
              "const jsonData = pm.response.json();",
              "let token = null;",
              "",
              "// Check for common token field names",
              "if (jsonData.access_token) {",
              "    token = jsonData.access_token;",
              "}",
              "",
              "// Store token in environment variable",
              "if (token) {",
              "    pm.environment.set(\"TOKEN\", token);",
              "}",
              "",
              "// Test to verify token was extracted and stored",
              "pm.test(\"Token extracted and stored successfully\", function () {",
              "    pm.expect(token).to.not.be.null;",
              "    pm.expect(pm.environment.get(\"TOKEN\")).to.eql(token);",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
